          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB-ADMIN-BUCKET}"
          },
          "query": "bypass =\n  from(bucket: \"admin_bucket\")\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n    |> filter(fn: (r) =>\n      r._measurement == \"device_status\" and\n      r._field == \"bypass\" and\n      r._value == \"1\"\n    )\n    |> last()\n    |> group()\n    |> count()\n    |> map(fn: (r) => ({ r with join_key: \"all\" }))\n\ntotal =\n  from(bucket: \"admin_bucket\")\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n    |> filter(fn: (r) =>\n      r._measurement == \"device_status\" and\n      r._field == \"bypass\"\n    )\n    |> last()\n    |> group()\n    |> count()\n    |> map(fn: (r) => ({ r with join_key: \"all\" }))\n\njoin(\n  tables: {bypass: bypass, total: total},\n  on: [\"join_key\"]\n)\n|> map(fn: (r) => ({\n  _value: float(v: r._value_bypass) / float(v: r._value_total) * 100.0,\n  _measurement: \"bypass_percentage\"\n}))",
