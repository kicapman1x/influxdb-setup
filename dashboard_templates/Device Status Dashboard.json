          "query": "from(bucket: \"admin_bucket\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) =>\n    r._measurement == \"device_status\" and\n    r._field == \"AllBypassFlight\" and\n    r.deviceId =~ /${deviceid:regex}/\n  )\n  |> map(fn: (r) => ({\n      r with\n      _value:\n        if r._value == \"1\" then 1\n        else if r._value == \"0\" then 0\n        else 0\n  }))",
